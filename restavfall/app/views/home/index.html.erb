<head>
    <%= stylesheet_link_tag 'home', {:media => "(min-height: 800px)"} %>
    <%= stylesheet_link_tag 'home_small', {:media => "(max-height: 800px)"} %>
    <%= javascript_include_tag 'home' %>
	
    <meta property="fb:app_id"             content="649498578495089" />
    <meta property="og:title"              content="Lychehjulet" />
    <meta property="og:description"        content="Finn lykken under uka!" />
    <meta property="og:image"              content="" />
</head>

<script> sr = "<%= @sr %>"; </script>

<div id="view">
    <div id="container" class="up">
		<%= image_tag 'cloud_1.png', {:id => "cloud-1", :class => "cloud"} %>
		<%= image_tag 'cloud_2.png', {:id => "cloud-2", :class => "cloud"} %>
		<%= image_tag 'cloud_1.png', {:id => "cloud-3", :class => "cloud"} %>
        <div id="h1" class="header">LYCHEHJULET</div>
        <div id="text_wrap">
            <div id="text" class="right">Tekst</div>
        </div>
        <div id="wheel">
            <div id="wheel_pin"><%= image_tag 'wheel_pin.png' %></div>
            <div id="wheel_board"><%= image_tag 'wheel_board.png' %></div>
            <div id="wheel_foot"><%= image_tag 'wheel_foot.png' %></div>
        </div>
        <div id="button">
            <div id="button_arrow"><%= image_tag 'button_arrow.png' %></div>
            <div id="button_click" onClick="spin();" ><%= image_tag 'button_click.png' %></div>
            <div id="button_foot"><%= image_tag 'button_foot.png' %></div>
            <div id="button_text" class="nudge">SPINN!</div>
        </div>
        <div id="rhino">
            <div id="rhino_belly" class="breathe_start"><%= image_tag "nesehorn_belly.png" %></div>
            <div id="rhino_feet"><%= image_tag "nesehorn_feet.png" %></div>
            <div id="rhino_tail" class="wave"><%= image_tag "nesehorn_hale.png" %></div>
            <div id="rhino_head"><%= image_tag "nesehorn_hode.png" %></div>
            <div id="rhino_head" class="rhino-blink"><%= image_tag "nesehorn_blunk2.png" %></div>
        </div>
        <div id="uka">
            <%= image_tag "UKElogo_hvit.png" %>
        </div>
        <div id="event">
			<div id="event-image-wrapper">
				<img id="event-image">
			</div>
			<div id="event-title">ACTION BRONSON</div>
        </div>
        <div id="profiles">
            <div id="friend">
				<img id="friend-image"><br>
				<div id="friend-name-div">
                <a id="friend-name" target="_blank">Friend</a>
				</div>
            </div>
            <div id="me">
				<img id="me-image"><br>
				<div id="me-name-div">
                <a id="me-name" target="_blank">Me</a>
				</div>
            </div>
        </div>
        <div id="description-wrapper">
            <div id="description">
                djkvmdk ksdmf ksdmf ldcmk ddkmf d
                cdsmnkf dfk d ddm fkmdck mkdmc
                nknc dkmv kdsv kd vkmkdv mkdvm
                dkmv dmvk mkvk mkkvm
            </div>
            <div id="ticket">
				<a id="ticket-link", target="_blank">KJØP BILLETT</a>
            </div>
        </div>
        <div id="button-wrapper">
            <div class="button3">
                <div class="inner-button" >
                    <%= image_tag "replay.png", {
                                  :id => "replay-img",  
                                  :class => "button-img", 
								  :title => "Prøv igjen",
								  :onClick => "again();"}  %> 
                </div>
            </div>
            <div class="button3">
                <div class="inner-button" >
					<%= image_tag "share.png", {:id => "share-image", :class => "button-img", :title => "Del på tidslinje"} %> 
                </div>
            </div>
            <div class="button3">
                <div class="inner-button" >
					<%= image_tag "message.png", {:id => "send-image", :class => "button-img", :title => "Del i chat"}%> 
                </div>
            </div>
        </div>
    </div>
</div>
